{
    "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
    "buildCommand": "next build",
    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": ["lhr1"],
    "public": false,
    "headers": [
      {
        "source": "/fonts/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }